{
  "version": 2,
  "projects": {
    "frontend": {
      "src": "client",
      "outputDirectory": "out"
    },
    "backend": {
      "src": "server",
      "use": "@vercel/node",
      "routes": [
        {
          "src": "/api/(.*)",
          "dest": "/index.js"
        }
      ]
    }
  },
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "server/package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server/index.js"
    }
  ]
}
